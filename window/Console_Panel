package window;

import java.awt.Graphics;

import javax.swing.JPanel;

public class Console_Panel extends JPanel{
	public int money;
	public int gas;
	public Console_Panel(int money,int gas) {
		this.money=money;
		this.gas=gas;
	}
	public void paint(Graphics g) {
		g.drawString("Money:"+this.money, 800, 10);
		g.drawString("Gas:"+this.gas, 800, 40);
	}
}
